// fibonacci.drg
// print first one hundred numbers in the fibonacci sequence

funcao fibonacci(interacoes) {
  var a = 0;
  var b = 1;
  para (var i = 0; i < interacoes; i = i + 1) {
    var temp = a + b;
    a = b;
    b = temp;
    escreva(temp);
  }

  retorna interacoes;
}

var a = fibonacci(100);
escreva(a);